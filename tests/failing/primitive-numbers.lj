// Integers

a() {
    return a = 1 - 1
}

b() {
    return b = 10 - 19
}

c() {
    return c = 1 - -1
}

test a() == 0
test b() == -9
test c() == 2

// Reals

d() {
    return d = 1.0 - 1.0
}

e() {
    return e = 10.5 - 19.5
}

f() {
    return f = 1 - 0.9999
}

test d() == 0
test e() == -9
test f() == 0.0001
