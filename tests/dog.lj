Dog
{
    private name = ""
    private bark = ""

    Dog (name, bark) {
        this.name = name
        this.bark = bark
    }

    public bark () {
        return format("%s says %s", this.name, this.bark)
    }

    public run () {
        return format("See %s run!", this.name)
    }
}

DukeDog extends Dog
{
    DukeDog() {
        super("Duke Dog", "GO DUKES!")
    }
}

dog() {
    return Dog("Spot", "Bark!")
}

dukeDog() {
    return DukeDog()
}

test dog().bark() == "Spot says Bark!"
test dukeDog().bark() == "Duke Dog says GO DUKES!"
