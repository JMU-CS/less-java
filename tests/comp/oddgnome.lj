main() {
    println(oddGnome([1,2,3,7,4,5]))
}

oddGnome(gnomes) {
    lastGnome = gnomes.get(0)
    og = gnomes.get(0)
    for (i: 1 -> gnomes.size()) {
        if (gnomes.get(i) != lastGnome + 1) {
            og = gnomes.get(i)
            break
        }

        lastGnome = gnomes.get(i)
    }

    return og
}

test oddGnome([1,2,3,7,4,5]) == 6
