oddGnome(gnomes) {
    lastGnome = gnomes[0]
    og = gnomes[0]
    for (i: 1 -> size(gnomes)) {
        if (gnomes[i] != lastGnome + 1) {
            og = gnomes[i]
            break
        }

        lastGnome = gnomes[i]
    }

    return og
}

test oddGnome([1,2,3,7,4,5]) == 7

print(oddGnome([1,2,3,7,4,5]))
